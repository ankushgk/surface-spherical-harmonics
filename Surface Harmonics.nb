            SubscriptBox[
             StyleBox["Y", "TI"], 
             RowBox[{
               StyleBox["l", "TI"], 
               StyleBox["m", "TI"]}]]}], 
          SqrtBox[
           RowBox[{
             StyleBox["l", "TI"], "(", 
             RowBox[{
               StyleBox["l", "TI"], "+", "1"}], ")"}]]], "    ", ";    ", 
         SubscriptBox["\[Phi]", 
          RowBox[{
            StyleBox["l", "TI"], 
            StyleBox["m", "TI"]}]], "\[LongEqual]", 
         OverscriptBox[
          StyleBox["r", "TI"], "^"], "\[Cross]", 
         SubscriptBox[
          StyleBox["Y", "TI"], 
